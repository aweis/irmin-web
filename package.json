{
  "name": "irmin-web",
  "version": "0.0.0",
  "description": "Use irmin in a ReasonML project", 
  "scripts": { 
    "buildjs": "js_of_ocaml --pretty #{self.target_dir}/default/executable/IrminWebApp.bc"
  },
  "esy": {
    "build": "refmterr dune build -p irmin-web",
    "release": {
      "releasedBinaries": [
        "IrminWebApp.exe"
      ]
    }
  },
   "buildDirs": {
    "library": {
      "name": "irmin-web.lib",
      "namespace": "IrminWeb",
      "preprocess": ["pps js_of_ocaml-ppx"],
      "require": ["irmin", "js_of_ocaml"]
    },
    "executable": {
      "require": ["digestif.ocaml", "js_of_ocaml", "irmin-web.lib", "irmin-mem", "irmin"],
      "main": "IrminWebApp",
      "name": "IrminWebApp.exe"
    }
  },
  "dependencies": {
    "@opam/dune": ">=1.6.0",
    "@opam/base64": "*",
    "@opam/cstruct": ">= 1.7.0",
    "@opam/digestif": "*",
    "@opam/irmin": "*",
    "@opam/irmin-mem": "*",
    "@opam/js_of_ocaml": ">= 2.6 < 3.4.0",
    "@opam/js_of_ocaml-ppx": "*",
    "@opam/lwt": "*",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": "*",
    "pesy": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  },
  "resolutions": {
    "@opam/irmin": "mirage/irmin:irmin.opam#8c94ce2ecef64921f9c3494f4e4ea312883cca10",
    "@opam/irmin-mem": "mirage/irmin:irmin-mem.opam#8c94ce2ecef64921f9c3494f4e4ea312883cca10"
  }
}
